    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "^8.0.4",
