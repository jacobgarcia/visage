    "eslint-plugin-jsx-a11y": "^6.1.2",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "hpp": "^0.2.2",
    "json2csv": "^4.3.1",
    "jsonwebtoken": "^8.4.0",
    "lodash.camelcase": "^4.3.0",
    "mime": "^2.4.0",
    "mongoose": "^5.3.14",
    "multer": "^1.4.1",
    "pm2": "^3.2.2",
    "react": "^16.6.3",
    "request-promise": "^4.2.2",
    "winston": "^3.1.0"
